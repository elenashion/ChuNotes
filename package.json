{
  "name": "chunotes",
  "productName": "ChuNotes",
  "version": "1.0.0",
  "description": "Messenger-like notebook",
  "main": "dist/src/background/index.js",
  "scripts": {
    "compile": "vue-tsc --noEmit && vite build",
    "serve": "vite",
    "ts": "tsc",
    "build": "npm run compile -- --outDir docs/webapp",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "electron:compile": "tsc && tsc-alias && webpack",
    "electron:serve": "npm run electron:compile && concurrently --kill-others vite \"electron .\" \"tsc --sourceMap -w\" \"tsc-alias -w\"",
    "electron:build": "npm run compile && npm run electron:compile && electron-builder",
    "electron:preview": "npm run compile && npm run electron:compile && electron ."
  },
  "author": "elena_shion",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.2.25",
    "vuetify": "^3.1.10"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^7.5.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-store": "^8.1.0",
    "eslint": "^8.26.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "terser-webpack-plugin": "^5.3.6",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.6.4",
    "vite": "^4.0.2",
    "vue-tsc": "^1.0.9",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
